x = int(input("Enter the number:"))
n = int(input("Enter N value:"))
factors = [i for i in range(1, x) if x % i == 0]
print("The factors of", x, "are:", factors)
print("Number of factors:", len(factors))
if n > len(factors):
    print("Invalid")
else:
    print(f"First {n} factors:", *factors[:n])  
